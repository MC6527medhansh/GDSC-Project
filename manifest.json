{
    "manifest_version": 3,
    "name": "ezcal",
    "version": "1.0.0",
    "action": { "default_popup": "index.html" },
    "permissions": ["storage", "tabs", "activeTab", "scripting"],
    "background": {
        "service_worker": "src/scripts/background.ts",
        "matches": ["<all_urls>"]
    }
  }