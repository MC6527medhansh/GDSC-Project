{
    "manifest_version": 3,
    "name": "Screenshot-to-Calendar",
    "version": "1.0",
    "description": "Take a screenshot of the current tab and send to Gumloop.",
    "permissions": [
      "tabs",           
      "storage",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  