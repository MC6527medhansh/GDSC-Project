{
    "manifest_version": 3,
    "name": "GDSC proejct",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "**/*",
          "*"
        ],
        "use_dynamic_url": false
      }
    ]
  }
  